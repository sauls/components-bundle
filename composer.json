{
  "name": "sauls/components-bundle",
  "description": "All Sauls components bundle for Symfony framework",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Saulius Vaičeliūnas",
      "email": "saulius@vaiceliunas.lt"
    }
  ],
  "require": {
    "php": ">=7.4",
    "symfony/framework-bundle": "^4.0 || ^5.0",
    "symfony/twig-bundle": "^4.0 || ^5.0",
    "sauls/helpers": "^1.0",
    "sauls/collections": "^1.0",
    "sauls/options-resolver": "^1.0",
    "sauls/widget": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sauls\\Bundle\\Components\\": "src/"
    },
    "exclude-from-classmap": [
      "../tests"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sauls\\Bundle\\Components\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "v1.x-dev"
    }
  }
}
